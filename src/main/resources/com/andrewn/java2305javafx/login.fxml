<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER"
      prefHeight="400.0" prefWidth="600.0"
      spacing="10.0"
      xmlns="http://javafx.com/javafx/11.0.2"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.andrewn.java2305javafx.LoginController">
   <children>
      <TextField fx:id="loginField" maxWidth="200.0" promptText="Login" />
      <PasswordField fx:id="passwordField" maxWidth="200.0" promptText="Password" />
      <Button alignment="CENTER" mnemonicParsing="false" onAction="#login" text="Log in" />
      <Label fx:id="errorLabel" style="-fx-text-fill: red;" />
   </children>
</VBox>
